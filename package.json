{
  "name": "zod-to-from",
  "version": "0.0.0",
  "description": "A collection of to/from adapters that convert between I/O formats and Zod-validated objects",
  "repository": "unjs/zod-to-from",
  "license": "MIT",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": "./src/index.mjs",
    "./cli": "./src/cli/cli.mjs"
  },
  "bin": {
    "ztf": "./src/cli/cli.mjs"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "build": "echo \"Build-less by design - using .mjs files directly\"",
    "dev": "vitest dev",
    "lint": "eslint . && prettier -c .",
    "lint:fix": "automd && eslint . --fix && prettier -w .",
    "prepack": "pnpm build",
    "release": "pnpm test && changelogen --release && npm publish && git push --follow-tags",
    "test": "pnpm lint && pnpm test:types && vitest run --coverage",
    "test:fast": "vitest run --coverage --reporter=basic",
    "test:parallel": "vitest run --coverage --maxConcurrency=8",
    "test:adapters": "vitest run test/adapters --coverage --maxConcurrency=8",
    "test:unit": "vitest run test/unit --coverage",
    "test:integration": "vitest run test/integration --coverage",
    "test:e2e": "vitest run test/e2e --coverage",
    "test:ai": "ENABLE_OLLAMA_TESTS=true vitest run test/adapters/ai.test.mjs",
    "test:ai:watch": "ENABLE_OLLAMA_TESTS=true vitest dev test/adapters/ai.test.mjs",
    "test:watch": "vitest dev",
    "test:types": "echo \"Type checking via JSDoc - no TypeScript compilation needed\""
  },
  "dependencies": {
    "ai": "^5.0.49",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.4.4",
    "jsonld": "^8.2.1",
    "n3": "^1.17.0",
    "ollama-ai-provider-v2": "^1.3.1",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "optionalDependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "apache-arrow": "^15.0.0",
    "avsc": "^5.7.7",
    "better-sqlite3": "^11.6.0",
    "docker-file-parser": "^2.0.1",
    "docxtemplater": "^3.47.0",
    "exceljs": "^4.4.0",
    "exifr": "^7.1.3",
    "gray-matter": "^4.0.3",
    "hcl2-parser": "^0.3.1",
    "ical.js": "^1.5.0",
    "ini": "^4.1.1",
    "js-yaml": "^4.1.0",
    "jszip": "^3.10.1",
    "mammoth": "^1.6.0",
    "node-id3": "^0.2.6",
    "nunjucks": "^3.2.4",
    "parquets": "^0.1.0",
    "parse-curl": "^0.2.1",
    "pdf-lib": "^1.17.1",
    "pdf-parse": "^1.1.1",
    "postal-mime": "^2.0.0",
    "pptxgenjs": "^3.12.0",
    "protobufjs": "^7.2.5",
    "puppeteer": "^21.11.0",
    "rdfxml-streaming-parser": "^0.1.0",
    "tar-stream": "^3.1.7",
    "toml": "^3.0.0",
    "topojson-client": "^3.1.0",
    "vcard4": "^0.1.0",
    "wellknown": "^0.5.0",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^3.2.4",
    "automd": "^0.4.0",
    "changelogen": "^0.6.2",
    "citty-test-utils": "^0.5.1",
    "eslint": "^9.33.0",
    "eslint-config-unjs": "^0.5.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0"
}
